{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build src",
    "create-post": "node ./scripts/createPost",
    "deploy": "yarn run export && touch out/.nojekyll && gh-pages --remote pages -t -d out",
    "dev": "next dev src",
    "export": "yarn build && next export src -o out",
    "serve": "http-server out",
    "start": "next start src"
  },
  "dependencies": {
    "@zeit/next-sass": "1.0.1",
    "dateformat": "3.0.3",
    "gray-matter": "4.0.2",
    "http-server": "0.11.1",
    "next": "9.1.4",
    "node-sass": "4.13.0",
    "raw-loader": "3.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-markdown": "4.2.2",
    "react-syntax-highlighter": "11.0.2"
  },
  "devDependencies": {
    "esm": "3.2.25",
    "gh-pages": "2.1.1",
    "lodash": "4.17.15",
    "slugify": "1.3.6"
  }
}
